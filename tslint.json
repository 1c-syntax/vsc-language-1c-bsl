{
    "extends": [
        "tslint:latest",
        "tslint-sonarts"
    ],
    "rules": {
        "semicolon": true,
        "object-literal-sort-keys": false,
        "trailing-comma": [
            false
        ],
        "no-console": false,
        "forin": false,
        "no-var-requires": false,
        "no-implicit-dependencies": [true, "dev"],
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "deprecation": true
    }
}